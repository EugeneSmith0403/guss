{
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["VITE_API_URL"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"],
      "env": ["VITE_API_URL"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "test:e2e": {
      "outputs": ["coverage/**"]
    },
    "clean": {
      "cache": false
    }
  }
}

